{
  "name": "@airium/ui",
  "version": "0.2509.14",
  "description": "UI module using Astro SSG with React 19",
  "type": "module",
  "scripts": {
    "build": "astro build",
    "dev": "astro dev",
    "preview": "astro preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "echo 'Linting disabled - TypeScript compilation provides type checking'",
    "clean": "rm -rf dist .astro",
    "deploy": "npm run build && npx ampx deploy",
    "deploy:prod": "npm run build && npx ampx deploy --branch main",
    "deploy:staging": "npm run build && npx ampx deploy --branch staging"
  },
  "dependencies": {
    "@airium/shared": "*",
    "@astrojs/react": "4.3.1",
    "@astrojs/tailwind": "6.0.2",
    "@aws-amplify/ui-react": "6.12.0",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tabs": "1.1.13",
    "astro": "5.13.7",
    "aws-amplify": "6.15.6",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "graphql": "16.11.0",
    "graphql-tag": "2.12.6",
    "lucide-react": "0.544.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.13"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "eslint-plugin-astro": "1.3.1",
    "jsdom": "27.0.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}
